{
  "css.validate": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.validate": true,
  "tailwindCSS.experimental.classRegex": [
    "classNames\\(\"(.*)\"\\)",
    "const\\s+.*ClassName\\s*=\\s*\"([^']*)\""
  ],
  "tailwindCSS.lint.cssConflict": "ignore",
  "files.exclude": {
    ".cache": true,
    "node_modules": true,
    "public": true
  },
  "svg.preview.background": "transparent",
  "editor.formatOnSave": true,
}
