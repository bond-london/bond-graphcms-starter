{
  "name": "bond-graphcms-starter",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "description": "Bond London Gatsby Graphcms starter",
  "version": "1.0.0",
  "license": "0BSD",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn  develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --max-warnings 0 \"**/*.{js,jsx,ts,tsx}\"",
    "clean:generated": "rimraf graphcms-fragments src/generated",
    "css": "tailwind build -i src/styles/index.css -o built.css",
    "restart": "run-s clean:generated develop",
    "restart:full": "run-s clean clean:generated develop",
    "build:full": "run-s clean clean:generated build",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "migrate": "node migrate/migrate.js",
    "apitest": "ts-node src/api/test.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@bond-london/bond-tailwind-plugin": "^1.0.4",
    "@bond-london/gatsby-graphcms-components": "^1.12.0",
    "@bond-london/gatsby-plugin-cms-i18next": "^1.5.0",
    "@bond-london/gatsby-plugin-cookie-scripts": "^2.0.1",
    "@bond-london/gatsby-plugin-generate-typings": "^2.0.0",
    "@bond-london/gatsby-transformer-extracted-lottie": "^2.0.1",
    "@bond-london/gatsby-transformer-extracted-svg": "^2.0.0",
    "@bond-london/graphcms-rich-text": "^1.0.0",
    "@bond-london/simple-gatsby-source-graphcms": "^5.1.1",
    "@fontsource/poppins": "^4.5.0",
    "@loadable/component": "^5.15.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node": "^16.11.13",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.3.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby": "^4.3.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-gatsby-cloud": "^4.3.0",
    "gatsby-plugin-image": "^2.3.0",
    "gatsby-plugin-loadable-components-ssr": "^4.2.0",
    "gatsby-plugin-manifest": "^4.3.0",
    "gatsby-plugin-postcss": "^5.3.0",
    "gatsby-plugin-react-helmet": "^5.3.0",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sharp": "^4.3.0",
    "gatsby-plugin-sitemap": "^5.3.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.26",
    "gatsby-source-filesystem": "^4.3.0",
    "gatsby-transformer-sharp": "^4.3.0",
    "i18next": "^21.6.5",
    "lmdb-store": "^1.6.14",
    "lodash": "^4.17.21",
    "lottie-web": "^5.8.1",
    "node-fetch": "2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "pure-react-carousel": "^1.27.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.15.3",
    "react-responsive": "^9.0.0-beta.5",
    "react-select": "^5.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.1",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bond-london/bond-graphcms-starter"
  },
  "bugs": {
    "url": "https://github.com/bond-london/bond-graphcms-starter/issues"
  },
  "devDependencies": {
    "@graphcms/management": "^1.4.0",
    "graphcms-migrate": "^1.0.1",
    "hint": "^6.1.9",
    "ts-node": "^10.4.0",
    "husky": "^7.0.0"
  }
}
