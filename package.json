    "@bond-london/graphcms-rich-text": "^1.3.1",
    "@bond-london/simple-gatsby-source-graphcms": "^6.1.2",
    "@fontsource/poppins": "^4.5.8",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "gatsby": "^4.18.0",
    "gatsby-plugin-canonical-urls": "^4.18.0",
    "gatsby-plugin-gatsby-cloud": "^4.18.0",
    "gatsby-plugin-google-tagmanager": "^4.18.0",
    "gatsby-plugin-image": "^2.18.0",
    "gatsby-plugin-manifest": "^4.18.0",
    "gatsby-plugin-page-creator": "^4.18.0",
    "gatsby-plugin-postcss": "^5.18.0",
    "gatsby-plugin-react-helmet": "^5.18.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^4.18.0",
    "gatsby-plugin-sitemap": "^5.18.0",
    "gatsby-source-filesystem": "^4.18.0",
    "gatsby-transformer-sharp": "^4.18.0",
    "lottie-web": "^5.9.4",
    "pure-react-carousel": "^1.27.8",
    "react": "^18.1.0",
    "react-cookie-consent": "^7.4.1",
    "react-dom": "^18.1.0",
    "react-select": "^5.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bond-london/bond-graphcms-starter"
  },
  "bugs": {
    "url": "https://github.com/bond-london/bond-graphcms-starter/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@bond-london/bond-tailwind-plugin": "^1.5.0",
    "@graphql-eslint/eslint-plugin": "^3.10.4",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/gtag.js": "^0.0.10",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-remove-graphql-queries": "^4.18.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.2.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "gatsby-plugin-eslint": "^4.0.2",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.0.24",
