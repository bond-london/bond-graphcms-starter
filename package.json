{
  "name": "bond-graphcms-starter",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "description": "Bond London Gatsby Graphcms starter",
  "version": "1.0.0",
  "license": "0BSD",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "restart": "run-s clean develop",
    "rebuild": "run-s clean build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --rulesdir node_modules/gatsby/dist/utils/eslint-rules --config .custom.eslintrc.json --max-warnings 0 \"src/**/*.{js,jsx,ts,tsx}\" \"gatsby-*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@bond-london/gatsby-graphcms-components": "^3.3.1",
    "@bond-london/gatsby-transformer-extracted-lottie": "^3.0.4",
